<h1>New Article</h1>
<%= form_for :order, url: orders_path do |f| %>
  <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
  </p>

  <p>
      <%= f.label :award %><br>
      <%= f.text_field :award %>
  </p>    
  <p>
     <%= f.label :content %><br>
     <%= f.text_area :content %>
  </p>
  
  <p>
  <%= f.submit %>
  </p>
<% end %>
<%= link_to "Back", orders_path %>

