<h1>記事一覧</h1>
<%= link_to "New Order", new_order_path %>
<table>
  <tr>
   <th>Title</th>
   <th>u_id</th>
   <th colspan="1"></th>
  </tr>
<% @orders.each do |order| %>
  <tr>
    <td><%= order.title %></td>
    <td><%= order.user_id %></td>
    <td><%= link_to "Show", order_path(order) %></td>
  </tr>
<% end %>
</table>

